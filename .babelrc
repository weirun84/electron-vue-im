{
  "comments": false,
  "env": {
    "test": {
      "presets": [
        [
          "env",
          {
            "targets": { "node": 7 }
          }
        ],
        "stage-0"
      ],
      "plugins": [ "istanbul" ]
    },
    "main": {
      "presets": [
        [
          "env",
          {
            "targets": { "node": 7 }
          }
        ],
        "stage-0"
      ]
    },
    "renderer": {
      "presets": [
        [
          "env",
          {
            "modules": false
          }
        ],
        "stage-0"
      ]
    },
    "web": {
      "presets": [
        [
          "env",
          {
            "modules": false
          }
        ],
        "stage-0"
      ]
    }
  },
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [ "> 1%", "last 2 versions", "not ie <= 8" ]
        }
      }
    ],
    "stage-2"
  ],
  "plugins": [ "transform-runtime" ]
}
